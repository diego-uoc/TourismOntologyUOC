    <!-- https://uoc.edu/tourismOntology#Attraction -->


    <owl:Class rdf:about="https://uoc.edu/tourismOntology#Attraction">
        <rdfs:subClassOf rdf:resource="http://dbpedia.org/resource/Tourist_attraction"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://uoc.edu/tourismOntology#attractionID"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://uoc.edu/tourismOntology#name"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://uoc.edu/tourismOntology#rating"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>a place which draws visitors by providing something of interest or pleasure.</rdfs:comment>
        <rdfs:label>Attraction</rdfs:label>
    </owl:Class>
    


