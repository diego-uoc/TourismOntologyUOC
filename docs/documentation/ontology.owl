    <!-- https://uoc.edu/tourismOntology#City -->


    <owl:Class rdf:about="https://uoc.edu/tourismOntology#City">
        <rdfs:subClassOf rdf:resource="http://dbpedia.org/resource/City"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://uoc.edu/tourismOntology#hasAgency"/>
                <owl:someValuesFrom rdf:resource="https://uoc.edu/tourismOntology#TravelAgency"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://uoc.edu/tourismOntology#hasHotel"/>
                <owl:someValuesFrom rdf:resource="https://uoc.edu/tourismOntology#Hotel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://uoc.edu/tourismOntology#hasLocation"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
