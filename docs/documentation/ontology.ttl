###  https://uoc.edu/tourismOntology#hasAgency
:hasAgency rdf:type owl:ObjectProperty ;
           rdfs:domain :City ;
           rdfs:range :TravelAgency ;
           rdfs:comment "A property that relates a City to an Agency"@en ;
           rdfs:label "has agency" .


###  https://uoc.edu/tourismOntology#hasAttraction
:hasAttraction rdf:type owl:ObjectProperty ;
               rdfs:domain :City ;
               rdfs:range :Attraction ;
               rdfs:comment "A property that relates a City to an Attrraction"@en ;
               rdfs:label "has attraction" .


###  https://uoc.edu/tourismOntology#hasAttractionType
:hasAttractionType rdf:type owl:ObjectProperty ;
                   rdfs:domain :Attraction ;
                   rdfs:range :AttractionType ;
                   rdfs:comment "A property that relates an Attraction to an Attraction type"@en ;
                   rdfs:label "has attraction type" .


###  https://uoc.edu/tourismOntology#hasCompany
:hasCompany rdf:type owl:ObjectProperty ;
            rdfs:domain :Transport ;
            rdfs:range :Company ;
            rdfs:comment "A property that relates a Transport to a Company"@en ;
            rdfs:label "has company" .


###  https://uoc.edu/tourismOntology#hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
