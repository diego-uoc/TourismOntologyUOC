           rdfs:range xsd:string ;
           rdfs:comment "The telephone number of the agency"@en ;
           rdfs:label "telephone:" .


###  https://uoc.edu/tourismOntology#transportID
:transportID rdf:type owl:DatatypeProperty ;
             rdfs:domain :Transport ;
             rdfs:range xsd:integer ;
             rdfs:comment "The id of the transport"@en ;
             rdfs:label "transport i d:" .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/resource/City
<http://dbpedia.org/resource/City> rdf:type owl:Class ;
                                   rdfs:comment "A city" ;
                                   rdfs:label "City" .


###  http://dbpedia.org/resource/Hotel
<http://dbpedia.org/resource/Hotel> rdf:type owl:Class ;
                                    rdfs:comment "A hotel" ;
                                    rdfs:label "Hotel" .


###  http://dbpedia.org/resource/Restaurant
<http://dbpedia.org/resource/Restaurant> rdf:type owl:Class ;
                                         rdfs:comment "A restaurant" ;
                                         rdfs:label "Restaurant" .


###  http://dbpedia.org/resource/Tourist_attraction
<http://dbpedia.org/resource/Tourist_attraction> rdf:type owl:Class ;
                                                 rdfs:comment "A tourist attraction" ;
                                                 rdfs:label "Tourist_attraction" .


###  http://dbpedia.org/resource/Transport
<http://dbpedia.org/resource/Transport> rdf:type owl:Class ;
                                        rdfs:comment "A transport" ;
                                        rdfs:label "Transport" .


###  http://dbpedia.org/resource/Travel_agency
<http://dbpedia.org/resource/Travel_agency> rdf:type owl:Class ;
                                            rdfs:comment "A Travel agency" ;
                                            rdfs:label "Travel_agency" .


###  http://www.w3.org/2002/07/owl#Thing
owl:Thing rdfs:comment "Thing. Root clas." ;
          rdfs:label "Thing" .


###  https://uoc.edu/tourismOntology#Attraction
:Attraction rdf:type owl:Class ;
            rdfs:subClassOf <http://dbpedia.org/resource/Tourist_attraction> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :attractionID ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :name ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :rating ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ;
            rdfs:comment "a place which draws visitors by providing something of interest or pleasure." ;
            rdfs:label "Attraction" .


###  https://uoc.edu/tourismOntology#AttractionType
:AttractionType rdf:type owl:Class ;
                rdfs:comment "a place which draws visitors by providing something of interest or pleasure."@en ;
                rdfs:label "Attraction Type" .


###  https://uoc.edu/tourismOntology#BeachCity
:BeachCity rdf:type owl:Class ;
           rdfs:subClassOf :Recommendation ;
           rdfs:comment "A touristic city by the beach"@en ;
           rdfs:label "Beach city" .


###  https://uoc.edu/tourismOntology#City
:City rdf:type owl:Class ;
      rdfs:subClassOf <http://dbpedia.org/resource/City> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasAgency ;
                        owl:someValuesFrom :TravelAgency
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasHotel ;
                        owl:someValuesFrom :Hotel
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasLocation ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :cityID ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :name ;
                        owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ;
      rdfs:comment "A place where many people live"@en ;
      rdfs:label "City" .


###  https://uoc.edu/tourismOntology#Company
:Company rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :name ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ;
         rdfs:comment "a commercial business."@en ;
         rdfs:label "Company" .


###  https://uoc.edu/tourismOntology#CulturalCity
:CulturalCity rdf:type owl:Class ;
              rdfs:subClassOf :Recommendation ;
              rdfs:comment "A cultural touristic city"@en ;
              rdfs:label "Cultural city" .


###  https://uoc.edu/tourismOntology#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :eventID ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasDate ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :name ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ;
       rdfs:comment "a planned public or social occasion"@en ;
       rdfs:label "Event" .


###  https://uoc.edu/tourismOntology#EventType
:EventType rdf:type owl:Class ;
           rdfs:comment "The type of event"@en ;
           rdfs:label "Event Type" .


###  https://uoc.edu/tourismOntology#FoodType
:FoodType rdf:type owl:Class ;
          rdfs:comment "The type of food"@en ;
          rdfs:label "Food Type" .


###  https://uoc.edu/tourismOntology#GastronomicCity
:GastronomicCity rdf:type owl:Class ;
                 rdfs:subClassOf :Recommendation ;
                 rdfs:comment "A traditional gastronomic touristic city"@en ;
                 rdfs:label "Gastronomic city" .


###  https://uoc.edu/tourismOntology#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf <http://dbpedia.org/resource/Hotel> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasPriceNight ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hotelID ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :name ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :rating ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :stars ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ;
       rdfs:comment "an establishment providing accommodation, meals, and other services for travellers and tourists."@en ;
       rdfs:label "Hotel" .


###  https://uoc.edu/tourismOntology#HotelType
:HotelType rdf:type owl:Class ;
           rdfs:comment "The type of hotel"@en ;
           rdfs:label "Hotel Type" .


###  https://uoc.edu/tourismOntology#Location
:Location rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :latitude ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :longitude ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          rdfs:comment "a particular place or position."@en ;
          rdfs:label "Location" .


###  https://uoc.edu/tourismOntology#Recommendation
:Recommendation rdf:type owl:Class ;
                rdfs:comment "a suggestion or proposal as to the best course of action, especially one put forward by an authoritative body."@en ;
                rdfs:label "Recommendation" .


###  https://uoc.edu/tourismOntology#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:subClassOf <http://dbpedia.org/resource/Restaurant> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :averagePrice ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :name ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :rating ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :restaurantID ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ;
            rdfs:comment "a place where people pay to sit and eat meals that are cooked and served on the premises."@en ;
            rdfs:label "Restaurant" .


###  https://uoc.edu/tourismOntology#Transport
:Transport rdf:type owl:Class ;
           rdfs:subClassOf <http://dbpedia.org/resource/Transport> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :averageCost ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :transportID ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ;
           rdfs:comment "a system or means of conveying people or goods from place to place."@en ;
           rdfs:label "Transport" .


###  https://uoc.edu/tourismOntology#TransportType
:TransportType rdf:type owl:Class ;
               rdfs:comment "The type of transport"@en ;
               rdfs:label "Transport Type" .


###  https://uoc.edu/tourismOntology#TravelAgency
:TravelAgency rdf:type owl:Class ;
              rdfs:subClassOf <http://dbpedia.org/resource/Travel_agency> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :agencyID ;
                                owl:cardinality "1"^^xsd:nonNegativeInteger
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :name ;
                                owl:cardinality "1"^^xsd:nonNegativeInteger
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :telephone ;
                                owl:cardinality "1"^^xsd:nonNegativeInteger
                              ] ;
              rdfs:comment "a company or shop that makes travel arrangements for people. Tourism & holidays."@en ;
              rdfs:label "Travel Agency" .


