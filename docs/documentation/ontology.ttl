###  https://uoc.edu/tourismOntology#hasRestaurant
:hasRestaurant rdf:type owl:ObjectProperty ;
               rdfs:domain :City ;
               rdfs:range :Restaurant ;
               rdfs:comment "A property that relates a City to a Restaurant"@en ;
               rdfs:label "has restaurant" .


###  https://uoc.edu/tourismOntology#hasTransport
:hasTransport rdf:type owl:ObjectProperty ;
              rdfs:domain :City ;
              rdfs:range :Transport ;
              rdfs:comment "A property that relates a City to a Transport"@en ;
              rdfs:label "has transport" .


###  https://uoc.edu/tourismOntology#hasTransportType
:hasTransportType rdf:type owl:ObjectProperty ;
                  rdfs:domain :Transport ;
                  rdfs:range :TransportType ;
                  rdfs:comment "A property that relates a Transport to a Transport type"@en ;
                  rdfs:label "has transport type" .


#################################################################
#    Data properties
#################################################################

###  https://uoc.edu/tourismOntology#agencyID
:agencyID rdf:type owl:DatatypeProperty ;
          rdfs:domain :TravelAgency ;
          rdfs:range xsd:integer ;
          rdfs:comment "The id of the agency"@en ;
          rdfs:label "agency i d:" .


###  https://uoc.edu/tourismOntology#attractionID
:attractionID rdf:type owl:DatatypeProperty ;
              rdfs:domain :Attraction ;
              rdfs:range xsd:integer ;
              rdfs:comment "The id of the attraction"@en ;
              rdfs:label "attraction i d:" .


###  https://uoc.edu/tourismOntology#averageCost
:averageCost rdf:type owl:DatatypeProperty ;
             rdfs:domain :Transport ;
             rdfs:range xsd:double ;
             rdfs:comment "The average cost of the transport"@en ;
             rdfs:label "average cost:" .


###  https://uoc.edu/tourismOntology#averagePrice
:averagePrice rdf:type owl:DatatypeProperty ;
              rdfs:domain :Restaurant ;
              rdfs:range xsd:integer ;
              rdfs:comment "The average price of the restaurant"@en ;
              rdfs:label "average price:" .


###  https://uoc.edu/tourismOntology#cityID
:cityID rdf:type owl:DatatypeProperty ;
        rdfs:domain :City ;
        rdfs:range xsd:integer ;
        rdfs:comment "The id of the city"@en ;
        rdfs:label "city i d:" .


###  https://uoc.edu/tourismOntology#eventID
:eventID rdf:type owl:DatatypeProperty ;
         rdfs:domain :Event ;
         rdfs:range xsd:integer ;
         rdfs:comment "The id of the event"@en ;
         rdfs:label "event i d:" .


###  https://uoc.edu/tourismOntology#hasDate
:hasDate rdf:type owl:DatatypeProperty ;
         rdfs:domain :Event ;
         rdfs:range xsd:date ;
         rdfs:comment "The date of the event"@en ;
         rdfs:label "has date:" .


###  https://uoc.edu/tourismOntology#hasPriceNight
:hasPriceNight rdf:type owl:DatatypeProperty ;
               rdfs:domain :Hotel ;
               rdfs:range xsd:integer ;
               rdfs:comment "The price per night of the hotel"@en ;
               rdfs:label "has price night:" .


###  https://uoc.edu/tourismOntology#hotelID
:hotelID rdf:type owl:DatatypeProperty ;
         rdfs:domain :Hotel ;
         rdfs:range xsd:integer ;
         rdfs:comment "The hotel id"@en ;
         rdfs:label "hotel i d:" .


###  https://uoc.edu/tourismOntology#latitude
:latitude rdf:type owl:DatatypeProperty ;
          rdfs:domain :Location ;
          rdfs:range xsd:double ;
          rdfs:comment "The latitude of the location"@en ;
          rdfs:label "latitude:" .


###  https://uoc.edu/tourismOntology#longitude
:longitude rdf:type owl:DatatypeProperty ;
           rdfs:domain :Location ;
           rdfs:range xsd:double ;
           rdfs:comment "The longitude of the location"@en ;
           rdfs:label "longitude:" .


###  https://uoc.edu/tourismOntology#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      rdfs:comment "The name of the individual"@en ;
      rdfs:label "name:" .


###  https://uoc.edu/tourismOntology#rating
:rating rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:double ;
        rdfs:comment "The rating of that individual"@en ;
        rdfs:label "rating:" .


###  https://uoc.edu/tourismOntology#restaurantID
:restaurantID rdf:type owl:DatatypeProperty ;
              rdfs:domain :Restaurant ;
              rdfs:range xsd:integer ;
              rdfs:comment "The id of the restaurant"@en ;
              rdfs:label "restaurant i d:" .


###  https://uoc.edu/tourismOntology#stars
:stars rdf:type owl:DatatypeProperty ;
